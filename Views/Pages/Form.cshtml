@model MusicShopForm

@{
    ViewBag.Title = "Form";
}

<h1>Create CD</h1>
<hr />
@using (Html.BeginForm("CreateNewEntity", "Pages", FormMethod.Post, new { @class = "form-horizontal" }))
{
