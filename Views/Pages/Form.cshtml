        @Html.LabelFor(x => x.CD.CD_artist_name.autograph, "Autograph", new { @class = "col-sm-2 col-form-label" })

        <div class="col-sm-10">
            @Html.EditorFor(x => x.CD.CD_artist_name.autograph, new { @class = "form-control" })
        </div>

    </div>

    <div class="form-group row">
        @Html.LabelFor(x => x.CD.CD_album_name.CD_album_name, "Album name", new { @class = "col-sm-2 col-form-label" })
        <div class="col-sm-10">
            @Html.EditorFor(x => x.CD.CD_album_name.CD_album_name, new { @class = "form-control" })
        </div>
    </div>

    <div class="form-group row">
        @Html.LabelFor(x => x.CD.CD_album_name.number_songs, "Number songs", new { @class = "col-sm-2 col-form-label" })

        <div class="col-sm-10">
            @Html.EditorFor(x => x.CD.CD_album_name.number_songs, new { @class = "form-control" })
        </div>

    </div>

    <div class="form-group row">
        @Html.LabelFor(x => x.CD.CD_label_name, "Label name", new { @class = "col-sm-2 col-form-label" })
        <div class="col-sm-10">
            @Html.EditorFor(x => x.CD.CD_label_name, new { @class = "form-control" })
        </div>
    </div>

    <div class="form-group row">
        @Html.LabelFor(x => x.CD.CD_release_date, "Release Date", new { @class = "col-sm-2 col-form-label" })
        <div class="col-sm-10">
            @Html.EditorFor(x => x.CD.CD_release_date, new { @class = "form-control" })
        </div>
    </div>

    <div class="form-group row">
        @Html.LabelFor(x => x.CD.CD_price, "Price", new { @class = "col-sm-2 col-form-label" })
        <div class="col-sm-10">
            @Html.EditorFor(x => x.CD.CD_price, new { @class = "form-control" })
