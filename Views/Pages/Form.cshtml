        @Html.LabelFor(x => x.CD.CD_image, "Image", new { @class = "col-sm-2 col-form-label" })
        <div class="col-sm-10">
            @Html.EditorFor(x => x.CD.CD_image, new { @class = "form-control" })
        </div>
    </div>

    <input type="submit" class="btn btn-success btn-submit-form" value="CREATE">
}
<hr />

