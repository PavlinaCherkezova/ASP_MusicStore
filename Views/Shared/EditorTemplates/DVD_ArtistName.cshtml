@model DVD_ArtistName
<form class="form-horizontal">

    <div class="form-group row">
        @Html.LabelFor(x => x.DVD_artist_name, "Artist name", new {@class = "col-sm-2 col-form-label"})
        <div class="col-sm-10">
            @Html.EditorFor(x => x.DVD_artist_name, new { @class =  "form-control" })
        </div>
    </div>

    <div class="form-group row">
        @Html.LabelFor(x => x.autograph, "Autograph", new {@class = "col-sm-2 col-form-label"})

        <div class="col-sm-10">
            @Html.EditorFor(x => x.autograph, new { @class =  "form-control" })
        </div>
            
    </div> 
</form>